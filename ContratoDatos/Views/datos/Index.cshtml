        <td>
            @Html.DisplayFor(modelItem => item.ParadaNombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descrip)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Parada_Lon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Parada_Lat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Parada_Cod)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Boarding)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ParadaId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ParadaId }) |
